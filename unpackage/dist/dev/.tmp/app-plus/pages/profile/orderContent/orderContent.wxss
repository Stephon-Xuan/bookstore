.order-container {
  padding: 10rpx 90rpx;
  background-color: #f7f7f6;
  margin-bottom: 20rpx;
}
.order-container .order-container-column .order-container-column-textone {
    font-size: 18rpx;
    color: #a7a7a7;
}
.order-container .order-container-column .order-container-column-texttwobar {
    display: -webkit-inline-box;
    display: -webkit-inline-flex;
    display: -ms-inline-flexbox;
    display: inline-flex;
    width: 71rpx;
    height: 30rpx;
    border-radius: 8rpx;
    border: solid 1rpx #fcb365;
}
.order-container .order-container-column .order-container-column-texttwobar .order-container-column-texttwobar-text {
      font-family: MicrosoftYaHei;
      font-size: 15rpx;
      font-weight: normal;
      font-stretch: normal;
      letter-spacing: 0rpx;
      color: #fcb365;
      text-align: center;
      line-height: 30rpx;
}
.order-container .order-container-column .order-container-column-textthree {
    font-size: 24rpx;
    color: #f5878a;
}
.order-container .order-container-main {
    background-color: #f0efef;
    border-radius: 9rpx;
    border: solid 1rpx #d3cbe2;
    padding: 20rpx 10rpx;
    margin-top: 10rpx;
}
.order-container .order-container-main .user {
      height: 50rpx;
}
.order-container .order-container-main .user .user-imgbox .user-img {
        width: 32rpx;
        height: 32rpx;
        border-radius: 50%;
}
.order-container .order-container-main .user .user-text {
        margin-left: 10rpx;
        height: 32rpx;
        line-height: 32rpx;
        font-size: 20rpx;
}
.order-container .order-container-main .user .jump {
        margin-left: 20rpx;
}
.order-container .order-container-main .user .jump .jump-img {
          width: 32rpx;
          height: 32rpx;
}
.order-container .order-container-main .book {
      padding: 20rpx 0;
      border-bottom: 1rpx solid #c8c7cc;
}
.order-container .order-container-main .book .book-imgbox {
        height: 100rpx;
        margin-right: 24rpx;
}
.order-container .order-container-main .book .book-imgbox .book-imgbox-img {
          width: 100rpx;
          height: 100rpx;
}
.order-container .order-container-main .book .book-detailbox {
        height: 100rpx;
}
.order-container .order-container-main .book .book-detailbox .book-detailbox-detail .book-detailbox-detail-sort {
          width: 250rpx;
}
.order-container .order-container-main .book .book-detailbox .book-detailbox-detail .book-detailbox-detail-price {
          width: 150rpx;
}
.order-container .order-container-main .total {
      padding: 5rpx 0;
}
.order-container .order-container-main .total .total-count {
        font-family: SourceHanSansCN-Medium;
        font-size: 19rpx;
        font-weight: normal;
        font-stretch: normal;
        line-height: 32rpx;
        letter-spacing: 0rpx;
}
.order-container .order-container-main .total .total-money {
        font-family: MicrosoftYaHei;
        font-size: 17rpx;
        font-weight: normal;
        font-stretch: normal;
        line-height: 32rpx;
        letter-spacing: 0rpx;
        color: #616161;
}
.order-container .order-container-main .total .total-money-active {
        font-family: MicrosoftYaHei;
        font-size: 17rpx;
        font-weight: normal;
        letter-spacing: 0rpx;
        color: #f58e65;
}
.order-container .order-container-main .rating .rating-box {
      width: 91rpx;
      height: 40rpx;
      background-color: #f2e4d5;
      border-radius: 9rpx;
}
.order-container .order-container-main .rating .rating-box .rating-box-text {
        font-family: MicrosoftYaHei;
        font-size: 16rpx;
        font-weight: normal;
        font-stretch: normal;
        line-height: 27rpx;
        letter-spacing: 0rpx;
        color: #f37b1d;
}

